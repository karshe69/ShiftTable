                <td id="draggable" class="rounded-md px-4 py-2" draggable="true"></td>
              </tr>
              <tr class="border-y border-gray-700">
                <td id="draggable" class="rounded-md px-4 py-2" draggable="true"></td>
                <td id="draggable" class="rounded-md px-4 py-2" draggable="true"></td>
                <td id="draggable" class="rounded-md px-4 py-2" draggable="true"></td>
                <td id="draggable" class="rounded-md px-4 py-2" draggable="true"></td>
                <td id="draggable" class="rounded-md px-4 py-2" draggable="true"></td>
                <td id="draggable" class="rounded-md px-4 py-2" draggable="true"></td>
                <td id="draggable" class="rounded-md px-4 py-2" draggable="true"></td>
              </tr>
              <tr class="border-y border-gray-700">
                <td id="draggable" class="rounded-md px-4 py-2" draggable="true" rowspan="2">asd
                dsa
                </td>
                <td id="draggable" class="rounded-md px-4 py-2" draggable="true" rowspan="2"></td>
                <td id="draggable" class="rounded-md px-4 py-2" draggable="true" rowspan="2"></td>
                <td id="draggable" class="rounded-md px-4 py-2" draggable="true" rowspan="2"></td>
                <td id="draggable" class="rounded-md px-4 py-2" draggable="true" rowspan="2"></td>
                <td id="draggable" class="rounded-md px-4 py-2" draggable="true" rowspan="2"></td>
