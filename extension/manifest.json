{
    "manifest_version": 3,
    "name": "ShiftTable",
    "description": "an app that helps you make tables for your shifts",
    "version": "1.0.0",
    "author": "plank",
    "icons": {
        "128": "shifttable logo.png"
    },
    "action": {
        "default_title": "Shift Table"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "identity",
        "identity.email"
    ],
    "host_permissions": ["https://api.com/*"]
    
}